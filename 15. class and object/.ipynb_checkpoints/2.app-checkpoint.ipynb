    "import math\n",
    "class Circle:\n",
    "    def __init__(self, r):\n",
    "        self.r = r \n",
    "    def area(self):\n",
    "        return math.pi * self.r **2\n",
    "        \n",
    "r = float(input(\"Enter radius : \"))\n",
    "\n",
    "c1 = Circle(r)\n",
    "\n",
    "c1.area()"
   ]
